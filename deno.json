{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/refs/heads/main/cli/schemas/config-file.v1.json",
  "name": "@nyoon/types",
  "version": "2.1.0",
  "exports": {
    "./flag": "./flag.ts",
    "./number": "./number.ts",
    "./string": "./string.ts"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "exclude": ["ban-types", "no-cond-assign", "no-explicit-any"]
    }
  },
  "tasks": {
    "test": "deno test --allow-all --no-check --parallel --doc"
  },
  "imports": {
    "@nyoon/base": "jsr:@nyoon/base@^2.1.1",
    "@std/assert": "jsr:@std/assert@^1.0.13"
  }
}
